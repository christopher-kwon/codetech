<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
    <typeAliases>
        <typeAlias alias="store" type="com.codetech.www.domain.Store"/>
        <typeAlias alias="menu" type="com.codetech.www.domain.Menu"/>
        <typeAlias alias="order" type="com.codetech.www.domain.Order"/>
        
        <!-- 2021-04-07 박인수 공지사항  -->
        <typeAlias alias="notice" type="com.codetech.www.domain.Notice"/>
    </typeAliases>

    <mappers>
        <mapper resource="sql/store.xml"/>
        <mapper resource="sql/menu.xml"/>
        <mapper resource="sql/order.xml"/>
        
        <!-- 2021-04-07 박인수 공지사항  -->
        <mapper resource="sql/notice.xml"/>
    </mappers>
</configuration>